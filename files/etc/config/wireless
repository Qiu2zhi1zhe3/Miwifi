
config wifi-device 'radio0'
	option type 'mac80211'
	option path 'pci0000:00/0000:00:00.0/0000:01:00.0'
	option band '5g'
	option country 'US'
	option hwmode '11a'
	option channel 'auto'
	option htmode 'VHT20'
	option legacy_rates '1'
	option mu_beamformer '0'

config wifi-iface 'default_radio0'
	option device 'radio0'
	option network 'lan'
	option mode 'ap'
	option ssid 'Stillness_5G'
	option encryption 'psk-mixed'
	option key '123456789'

config wifi-device 'radio1'
	option type 'mac80211'
	option path 'platform/10180000.wmac'
	option band '2g'
	option htmode 'HT20'
	option country 'US'
	option disabled '0'
	option channel '6'
	option hwmode '11g'
	option legacy_rates '1'
	option mu_beamformer '0'

config wifi-iface 'default_radio1'
	option device 'radio1'
	option network 'lan'
	option mode 'ap'
	option ssid 'Stillness'
	option encryption 'psk-mixed'
	option key '123456789'

config wifi-iface
	option ssid 'FPT Danh Dung'
	option encryption 'psk2'
	option device 'radio1'
	option mode 'sta'
	option bssid 'A8:25:EB:BA:A3:44'
	option key '87654321'
	option network 'lan wwan'

